            #binding-cells = <0>;
            bindings
                = <&macro_press &kp LCTRL>
                , <&macro_tap &kp BACKSPACE>
		, <&macro_release &kp LCTRL>
		;
	};
	M_redo: RedoLast{
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&macro_press &kp LCTRL>
                , <&macro_tap &kp Y>
		, <&macro_release &kp LCTRL>
		;
	};
	M_undo: UndoLast{
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&macro_press &kp LCTRL>
                , <&macro_tap &kp Z>
		, <&macro_release &kp LCTRL>
		;
	};
