        tri_layer {
            bindings = <
//              ╭───────────┬───────────┬───────────┬───────────┬───────────╮     ╭───────────┬───────────┬───────────┬───────────┬───────────╮
//╭─────────────┤    F1     │    F2     │    F3     │    F4     │           │     │ PROFILE 0 │ PROFILE 2 │ PROFILE 4 │INC BRIGHT │ VOL UP    ├───────────╮
