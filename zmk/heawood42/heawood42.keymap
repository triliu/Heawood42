        };
        combo_shift_right{
            timeout-ms = <50>;
            key-positions = <14 15>;
            bindings = <&kp RSHFT>;
        };
        combo_ctrl_left{
            timeout-ms = <50>;
            key-positions = <3 4>;
            bindings = <&kp LCTRL>;
        };
        combo_ctrl_right{
            timeout-ms = <50>;
            key-positions = <7 8>;
            bindings = <&kp RCTRL>;
        };
        combo_alt_left{
            timeout-ms = <50>;
            key-positions = <2 4>;
            bindings = <&kp LALT>;
        };
        combo_alt_right{
            timeout-ms = <50>;
            key-positions = <7 9>;
            bindings = <&kp RALT>;
        };
        combo_gui_left{
            timeout-ms = <50>;
