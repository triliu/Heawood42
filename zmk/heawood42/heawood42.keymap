#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>
#include <dt-bindings/zmk/outputs.h>
#include <dt-bindings/zmk/ext_power.h>
#include <dt-bindings/zmk/rgb.h>
#include <dt-bindings/led/led.h>
 
#define RTOG RGB_TOG
#define RON  RGB_ON
#define ROFF RGB_OFF
#define REFF RGB_EFF
#define RBRI RGB_BRI
#define RBRD RGB_BRD
#define RSAI RGB_SAI
#define RSAD RGB_SAD
#define RSPI RGB_SPI
#define RSPD RGB_SPD
#define REFF RGB_EFF
#define REFR RGB_EFR
#define RHUI RGB_HUI
#define RHUD RGB_HUD

#define BT0 &bt BT_SEL 0
#define BT1 &bt BT_SEL 1
#define BT2 &bt BT_SEL 2
#define BT3 &bt BT_SEL 3
#define BT4 &bt BT_SEL 4
#define BT5 &bt BT_SEL 5
#define WIN 0
#define LEF 1
#define RIG 2
#define TRI 3
#define NUM 4
#define LED 5
#define LBsp 6 
#define LMin 7
#define LFsl 8


&led_strip {
    chain-length = <35>;
};


// My LED pin is P0.06
// See https://nicekeyboards.com/docs/nice-nano/pinout-schematic/
&pinctrl {
    spi3_default: spi3_default {
        group1 {
            psels = <NRF_PSEL(SPIM_MOSI, 0, 6)>;
        };
    };
 
    spi3_sleep: spi3_sleep {
        group1 {
            psels = <NRF_PSEL(SPIM_MOSI, 0, 6)>;
            low-power-enable;
        };
    };
};

&spi3 {
  compatible = "nordic,nrf-spim";
  status = "okay";

  pinctrl-0 = <&spi3_default>;
  pinctrl-1 = <&spi3_sleep>;
  pinctrl-names = "default", "sleep";

  led_strip: ws2812@0 {
    compatible = "worldsemi,ws2812-spi";

    // SPI //
    reg = <0>; // ignored, but necessary for SPI bindings //
    spi-max-frequency = <4000000>;

    // WS2812 //
    chain-length = <35>; // number of LEDs //
    spi-one-frame = <0x70>;
    spi-zero-frame = <0x40>;
    color-mapping = <LED_COLOR_ID_GREEN
                          LED_COLOR_ID_RED
                          LED_COLOR_ID_BLUE>;
  };
};



&sk {
    release-after-ms = <2000>;
    quick-release;
    };


// See https://github.com/rastasheep/zmk-config/blob/master/config/lily58.keymap
#define HYP LS(LC(LA(LGUI)))
#define HYPER(key) LS(LC(LA(LG(key))))
/ {

    behaviors {
    	//Hold Taps
        ht: hold_tap { 
            compatible = "zmk,behavior-hold-tap";
            #binding-cells = <2>;
            flavor = "tap-preferred";
            tapping-term-ms = <220>;
            quick-tap-ms = <150>;
            bindings = <&kp>, <&kp>;
        };
        htl: hold_tap_layers { 
            compatible = "zmk,behavior-hold-tap";
            #binding-cells = <2>;
            flavor = "hold-preferred";
            tapping-term-ms = <220>;
            quick-tap-ms = <150>;
            bindings = <&mo>, <&tog>;
        };
	
	//Tap Dances
	LSpace: spc_ent_left {
	    compatible = "zmk,behavior-tap-dance";
            #binding-cells = <0>;
            tapping-term-ms = <200>;
            bindings = <&kp SPACE>, <&kp ENTER>;  
	};
	RSpace: spc_ent_right {
	    compatible = "zmk,behavior-tap-dance";
            #binding-cells = <0>;
            tapping-term-ms = <200>;
            bindings = <&kp SPACE>, <&kp ENTER>;  
	};
	EscL0: Esctab_ent {
	    compatible = "zmk,behavior-tap-dance";
            #binding-cells = <0>;
            tapping-term-ms = <200>;
            bindings = <&kp ESC>, <&to 0>;
	};
	L_CAG_TD: CtrAltGui_L_TD{
	    compatible = "zmk,behavior-tap-dance";
            #binding-cells = <0>;
            tapping-term-ms = <300>;
            bindings = <&kp LCTRL>, <&kp LALT>,  <&kp LGUI>;
	};
	R_AGC_TD: AltGuiCtr_R_TD{
	    compatible = "zmk,behavior-tap-dance";
            #binding-cells = <0>;
            tapping-term-ms = <300>;
            bindings = <&kp RALT>, <&kp RGUI>, <&kp RCTRL>;
	};
	TD_COMMA: comma_semi_tapdance{
	    compatible = "zmk,behavior-tap-dance";
            #binding-cells = <0>;
            tapping-term-ms = <200>;
            bindings = <&kp COMMA>, <&kp SEMI>;
	};
	TD_LT: LessThan_LParenthesis_tapdance{
	    compatible = "zmk,behavior-tap-dance";
            #binding-cells = <0>;
            tapping-term-ms = <200>;
            bindings = <&kp COMMA>, <&kp N9>;
	};
	TD_DOT: dot_colon_tapdance{
	    compatible = "zmk,behavior-tap-dance";
            #binding-cells = <0>;
            tapping-term-ms = <200>;
            bindings = <&kp DOT>, <&kp COLON>;
	};
	TD_GT: GreaterThan_RParenthesis_tapdance{
	    compatible = "zmk,behavior-tap-dance";
            #binding-cells = <0>;
            tapping-term-ms = <200>;
            bindings = <&kp DOT>, <&kp N0>;
	};

	

	//Morphs
	MM_DOT: DotGT_ColonRParen_Modmorph {
            compatible = "zmk,behavior-mod-morph";
            #binding-cells = <0>;
            bindings = <&TD_DOT>, <&TD_GT>;
            mods = <(MOD_LSFT | MOD_RSFT)>;
            keep-mods = <(MOD_LSFT | MOD_RSFT)>;
	};
	MM_COMMA: CommaLT_SemiLParenthesis_Modmorph {
            compatible = "zmk,behavior-mod-morph";
            #binding-cells = <0>;
            bindings = <&TD_COMMA>, <&TD_LT>;
            mods = <(MOD_LSFT | MOD_RSFT)>;
            keep-mods = <(MOD_LSFT | MOD_RSFT)>;
	};

    };  //end behaviours
    


