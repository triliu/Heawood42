    keymap {
        compatible = "zmk,keymap";
        default_layer {
            bindings = <
//              ╭───────────┬───────────┬───────────┬───────────┬───────────╮     ╭───────────┬───────────┬───────────┬───────────┬───────────╮
//╭─────────────┤     Q     │     W     │    E      │     R     │     T     │     │     Y     │     U     │     I     │     O     │     P     ├───────────╮
  &EscL0        &kp   Q     &kp   W     &kp  E      &kp   R     &kp   T           &kp   Y     &kp   U     &kp   I     &kp   O     &kp P       &lt LBsp MINUS
//│             ├───────────┼───────────┼───────────┼───────────┼───────────┤     ├───────────┼───────────┼───────────┼───────────┼───────────┤    -  (l) │
//├─────────────┤     A     │     S     │    D      │     F     │     G     │     │     H     │     J     │     K     │     L     │     ; (l) ├───────────┤
  &mt LSHFT TAB &kp   A     &kp   S     &kp  D      &kp   F     &kp   G           &kp   H     &kp   J     &kp   K     &kp   L     &lt LMin SEMI &lt RIG SQT
//│   LSHIFT    ├───────────┼───────────┼───────────┼───────────┼───────────┤     ├───────────┼───────────┼───────────┼───────────┼───────────┤ ' / Right │
//╰─────────────┤     Z     │     X     │    C      │     V     │     B     │     │     N     │     M     │    , <    │    . >   <│   / ? (l) ├───────────╯
                &mt LSHIFT Z &mt LCTRL X &mt LALT C &mt LGUI V  &kp   B           &kp   N     &mt RGUI M  &mt RALT COMMA &mt RCTRL DOT &mt RSHFT FSLH
//              ╰───────────┴───────────┴───────────┴───────────┴───────────╯     ╰───────────┴───────────┴───────────┴───────────┴───────────╯
//                          ╭───────────┬───────────┬───────────┬───────────╮     ╭───────────┬───────────┬───────────┬───────────╮
//                          │           │           │           │           │     │           │           │           │           │
                            &key_repeat &mo LEF     &kp ENTER   &kp TAB           &R_AGC_TD   &kp SPACE   &mo RIG    &kp N0
//                          ╰───────────┴───────────┤           ├───────────╯     ╰───────────┤           ├───────────┴───────────╯
//                                                  ╰───────────╯                             ╰───────────╯
		>;
	};


        left_layer {
            bindings = <
//              ╭───────────┬───────────┬───────────┬───────────┬───────────╮     ╭───────────┬───────────┬───────────┬───────────┬───────────╮
//╭─────────────┤     !     │     @     │     #     │     $     │     %     │     │     ^     │     &     │     *     │      (    │    )      ├───────────╮
  &EscL0        &kp EXCL    &kp  AT     &kp  HASH   &kp  DLLR   &kp  PRCNT        &kp CARET   &kp  AMPS   &kp STAR    &kp  LPAR   &kp  RPAR   &kp PLUS
//│ Esc + LayEsc├───────────┼───────────┼───────────┼───────────┼───────────┤     ├───────────┼───────────┼───────────┼───────────┼───────────┤           │
//├─────────────┤     1     │     2     │     3     │     4     │     5     │     │     6     │     7     │     8     │      9    │    0      ├───────────┤
  &studio_unlock &kp N1     &kp N2      &kp  N3     &kp  N4     &kp  N5           &kp  N6     &kp   N7    &kp   N8    &kp   N9    &kp  N0     &none
//│             ├───────────┼───────────┼───────────┼───────────┼───────────┤     ├───────────┼───────────┼───────────┼───────────┼───────────┤           │
//╰─────────────┤           │           │           │           │           │     │           │           │           │           │           ├───────────╯
                &kp LSHIFT  &kp LCTRL   &kp LALT    &kp LGUI    &bt BT_CLR        &none       &kp RGUI    &kp  RALT   &kp RCTRL   &kp RSHFT
//              ╰───────────┴───────────┴───────────┴───────────┴───────────╯     ╰───────────┴───────────┴───────────┴───────────┴───────────╯
//                          ╭───────────┬───────────┬───────────┬───────────╮     ╭───────────┬───────────┬───────────┬───────────╮
//                          │           │           │           │           │     │           │           │           │           │
                            &kp N0      &trans      &kp LSHFT   &L_CAG_TD         &kp RSHFT   &kp RSHFT   &trans      &kp N0 
//                          ╰───────────┴───────────┤           ├───────────╯     ╰───────────┤           ├───────────┴───────────╯
//          	                                      ╰───────────╯                             ╰───────────╯
		>;
	};

        right_layer {
            bindings = <
//              ╭───────────┬───────────┬───────────┬───────────┬───────────╮     ╭───────────┬───────────┬───────────┬───────────┬───────────╮
//╭─────────────┤     F2    │    F3     │     F4    │    F5     │     F6    │     │    F7     │    F8     │     F9    │    F10    │    F11    ├───────────╮
  &kp  F1       &kp   F2    &kp  F3     &kp   F4    &kp  F5     &kp   F6          &kp  F7     &kp  F8     &kp   F9    &kp  F10    &kp  F11    &kp  F12
//│    F1       ├───────────┼───────────┼───────────┼───────────┼───────────┤     ├───────────┼───────────┼───────────┼───────────┼───────────┤    F12
//├─────────────┤           │           │           │           │           │     │           │           │           │           │           ├───────────┤
  &mt LSHFT TAB &none       &none       &none       &none       &none             &none       &none       &none       &none       &none       &none
//│   LSHIFT    ├───────────┼───────────┼───────────┼───────────┼───────────┤     ├───────────┼───────────┼───────────┼───────────┼───────────┤           │
//╰─────────────┤           │           │           │           │           │     │           │           │           │           │           ├───────────╯
                &kp LSHIFT  &kp LCTRL   &kp LALT    &kp LGUI    &none             &none       &kp RGUI    &kp  RALT   &kp RCTRL   &kp RSHFT
//              ╰───────────┴───────────┴───────────┴───────────┴───────────╯     ╰───────────┴───────────┴───────────┴───────────┴───────────╯
//                          ╭───────────┬───────────┬───────────┬───────────╮     ╭───────────┬───────────┬───────────┬───────────╮
