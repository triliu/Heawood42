        };
	M_word_del: BDelWord{
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&macro_press &kp LCTRL>
                , <&macro_tap &kp DELETE>
		, <&macro_release &kp LCTRL>
		;
	};
	M_word_bsp: BSpcWord{
